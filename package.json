{
  "name": "react-compose-tools",
  "version": "0.1.0",
  "description": "A functional React higher-order component utility library",
  "author": "Alex Hunt <hello@alexhunt.io>",
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/huntie/react-compose-tools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huntie/react-compose-tools.git"
  },
  "bugs": {
    "url": "https://github.com/huntie/react-compose-tools/issues"
  },
  "engines": {
    "yarn": "1.16.0"
  },
  "scripts": {
    "build": "npm run build:typedefs && npm run build:babel",
    "build:babel": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:typedefs": "tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "typescript": "^3.5.1"
  },
  "keywords": [
    "react"
  ]
}
